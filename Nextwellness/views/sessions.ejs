    <style>
        /* Base styles */
        .session-container {
            display: flex;
            flex-wrap: wrap;
            justify-content: space-between;
        }

        .session-card {
            flex: 0 0 calc(50% - 40px);
            background-color: #59f964;
            border-radius: 8px;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 1);
            padding: 15px;
            margin-bottom: 20px;
        }
        h1 {
            text-align: center;
           
        }
        .profile-img {
          border-radius: 50%;
          width: 50px; /* Adjust size as needed */
          height: 50px; /* Adjust size as needed */
      }

        /* Responsive styles */
        @media (max-width: 1200px) {
            .session-card {
                flex: 0 0 calc(50% - 40px);
            }
        }

        @media (max-width: 768px) {
            .session-card {
                flex: 0 0 calc(100% - 30px);
                margin-left: 10px;
            }
        }
    </style> 
