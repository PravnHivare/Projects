    <script>
      document.addEventListener("DOMContentLoaded", function() {
  // Get the treatment dropdown link
  var treatmentDropdown = document.querySelector('.navbar-nav .dropdown .nav-link');

  // Get the treatment dropdown menu
  var treatmentDropdownMenu = document.querySelector('.navbar-nav .dropdown-menu');

  // Add click event listener to the treatment dropdown link
  treatmentDropdown.addEventListener('click', function(event) {
    // Prevent the default link behavior
    event.preventDefault();
    
    // Toggle the visibility of the treatment dropdown menu
    if (treatmentDropdownMenu.style.display === "block") {
      treatmentDropdownMenu.style.display = "none";
    } else {
      treatmentDropdownMenu.style.display = "block";
    }
  });
});

    </script>


